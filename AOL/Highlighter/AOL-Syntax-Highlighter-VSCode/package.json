{
    "name": "aol-syntax-highlighting",
    "displayName": "AOL Syntax Highlighting",
    "description": "Syntax highlighting for the AOL language",
    "version": "1.0.0",
    "publisher": "Pheonix Studios (AkshuDev)",
    "engines": {
      "vscode": "^1.75.0"
    },
    "categories": ["Programming Languages"],
    "contributes": {
      "languages": [
        {
          "id": "aol",
          "aliases": ["AOL", "aol"],
          "extensions": [".aol"],
          "configuration": "./aol-language.json"
        }
      ],
      "grammars": [
        {
          "language": "aol",
          "scopeName": "source.aol",
          "path": "./syntaxes/aol.tmLanguage.json"
        }
      ]
    }
  }
